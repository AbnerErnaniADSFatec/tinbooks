    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -23.1791, lng: -45.8872},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBUluBnkOM0SqCU9FkxScr3UYFl8gDevWI&callback=initMap"
    async defer></script>
  </body>
